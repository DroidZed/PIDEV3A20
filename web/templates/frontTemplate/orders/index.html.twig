<!DOCTYPE html>

<title>TblUserorder index</title>

{% block body %}
    <h1>TblUserorder index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Numberorder</th>
                <th>Createddtm</th>
                <th>Paydtm</th>
                <th>Orderaddress</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for tbl_userorder in tbl_userorders %}
            <tr>
                <td>{{ tbl_userorder.numberorder }}</td>
                <td>{{ tbl_userorder.createddtm ? tbl_userorder.createddtm|date('Y-m-d') : '' }}</td>
                <td>{{ tbl_userorder.paydtm ? tbl_userorder.paydtm|date('Y-m-d') : '' }}</td>
                <td>{{ tbl_userorder.orderaddress }}</td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
{% endblock %}
