<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="761.0" prefWidth="1422.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tn.nebulagaming.views.ManageQuizController">
   <children>
      <ImageView fitHeight="821.0" fitWidth="1440.0" layoutX="-12.0" layoutY="-30.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../assets/Quiz.png" />
         </image>
      </ImageView>
      <TableView fx:id="tvQuiz" layoutX="224.0" layoutY="251.0" prefHeight="380.0" prefWidth="992.0">
        <columns>
          <TableColumn fx:id="titleQuizCol" prefWidth="75.0" text="Title" />
          <TableColumn fx:id="descQuizCol" prefWidth="75.0" text="Description" />
            <TableColumn fx:id="startDateCol" prefWidth="75.0" text="Start Date" />
            <TableColumn fx:id="endDateCol" prefWidth="75.0" text="End Date" />
            <TableColumn fx:id="visibilityCol" prefWidth="75.0" text="Visibility" />
            <TableColumn fx:id="postedDateCol" prefWidth="75.0" text="Posted At" />
            <TableColumn fx:id="postedByCol" prefWidth="75.0" text="Posted By" />
            <TableColumn fx:id="correctAnswerCol" prefWidth="75.0" text="Correct Answer" />
            <TableColumn fx:id="idPostCol" prefWidth="75.0" text="idPost" visible="false" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button fx:id="btnAddQuiz" layoutX="222.0" layoutY="210.0" mnemonicParsing="false" text="Add new Quiz" />
      <Button fx:id="btnGoBack" layoutX="1147.0" layoutY="655.0" mnemonicParsing="false" text="Go Back" />
   </children>
</AnchorPane>
