      </ImageView>
      <VBox layoutX="918.0" layoutY="244.0" prefHeight="208.0" prefWidth="278.0" spacing="10.0">
         <children>
            <Button fx:id="btnManagePosts" mnemonicParsing="false" prefHeight="55.0" prefWidth="288.0" text="Manage Posts ">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
            <Button fx:id="btnManageGamification" mnemonicParsing="false" prefHeight="55.0" prefWidth="288.0" text="Manage Gamifications">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
            <Button fx:id="btnManageBadges" mnemonicParsing="false" prefHeight="55.0" prefWidth="288.0" text="Manage Badges">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
         </children>
      </VBox>
      <Button fx:id="btnBackAd" layoutX="990.0" layoutY="546.0" mnemonicParsing="false" onAction="#goToAdmin" prefHeight="37.0" prefWidth="130.0" text="Go Back">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Button fx:id="btnViewAsUser" layoutX="989.0" layoutY="500.0" mnemonicParsing="false" prefHeight="37.0" prefWidth="130.0" text="View as User">
